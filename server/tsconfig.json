{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext"],
    "target": "ESNext",
    "resolveJsonModule": true
  }
}
